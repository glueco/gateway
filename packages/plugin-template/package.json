{
  "name": "@glueco/plugin-template",
  "version": "0.0.0",
  "private": true,
  "description": "Template plugin for Personal Resource Gateway - demonstrates dual-entrypoint architecture (not published)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./proxy": {
      "types": "./dist/proxy.d.ts",
      "import": "./dist/proxy.mjs",
      "require": "./dist/proxy.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./contracts": {
      "types": "./dist/contracts.d.ts",
      "import": "./dist/contracts.mjs",
      "require": "./dist/contracts.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@glueco/shared": "^0.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@glueco/sdk": "^0.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@glueco/shared": "^0.1.0",
    "@glueco/sdk": "^0.1.0"
  },
  "peerDependenciesMeta": {
    "@glueco/sdk": {
      "optional": true
    }
  },
  "keywords": [
    "prg",
    "plugin",
    "template"
  ],
  "license": "MIT"
}
